<!DOCTYPE html>
<html lang="en" ng-app="mngApp">

<head>
  <!-- Public head info -->
  <%-include ../ejs/head-info.html  %>
    <!-- Public stylesheet -->
    <%-include ../ejs/head-links.html  %>
      <!-- Private stylesheet -->
      <link href="./clientMng_style.css" rel="stylesheet" type="text/css" />
</head>

<body ng-controller="mngCtrl">

  <!-- <div class="ipage-header" ng-include="pageInition.tplHeaderUrl"> -->
  <div class="ipage-header">
    <%-include ../ejs/ipage-header.html  %>
  </div>

  <div class="ipage-container">
    <!-- <div class="ipage-sidebar-wrapper" ng-include="pageInition.tplSidebarUrl"></div> -->
    <div class="ipage-sidebar-wrapper">
      <%-include ../ejs/ipage-sidebar.html  %>
    </div>
    <div class="ipage-content-wrapper">
      <div class="page-content">
        <!-- BEGIN CONTENT CONTAINER-->
        <div class="dyn_content" ng-view></div>
        <!-- END CONTENT CONTAINER-->
      </div>
    </div>
  </div>

  <!-- <div class="ipage-footer" ng-include="pageInition.tplFooterUrl"></div> -->
  <div class="ipage-footer">
    <%-include ../ejs/ipage-footer.html  %>
  </div>

  <!-- Public scripts -->
  <!-- It can be generated by 'fs' in the future. -->
  <div class="public-scripts">
    <%-include ../ejs/body-scripts.html  %>
  </div>

  <!-- Priviate scripts -->
  <!-- Can be wrote into slef's controller, but a entry .js need ti be kept-->
  <div class="private-scripts">
    <script src="./clientMng.js"></script>
  </div>

